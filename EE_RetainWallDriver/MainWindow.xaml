<Window x:Class="EE_RetainWallDriver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EE_RetainWallDriver"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <StackPanel Orientation="Horizontal">
                <StackPanel Orientation="Vertical">
                    <Label Content="Front Side Soil Depth" FontWeight="Bold" />
                    <TextBlock x:Name="tbFrontSideSoilDepth"/>
                </StackPanel>
                <StackPanel Orientation="Vertical">
                    <Label Content="Back Side Soil Depth" FontWeight="Bold" />
                    <TextBlock x:Name="tbBackSideSoilDepth"/>
                </StackPanel>
            </StackPanel>
        </StackPanel>
        <StackPanel Orientation="Vertical">
            <Label Content="Wall Stem" FontWeight="Bold" />
            <TextBlock x:Name="tbStemResults"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Ka" FontWeight="Bold" />
            <TextBlock x:Name="tbKa"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Kp" FontWeight="Bold" />
            <TextBlock x:Name="tbKp"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Gamma" FontWeight="Bold" />
            <TextBlock x:Name="tbGamma"/>
        </StackPanel>
    </StackPanel>
</Window>
